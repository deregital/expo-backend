{
  "name": "expo-backend-types",
  "version": "0.0.48",
  "description": "",
  "author": "Expo",
  "private": false,
  "license": "UNLICENSED",
  "files": [
    "dist/types",
    "dist/src/**/*.dto.{js,d.ts,ts}",
    "dist/src/**/*.schema.{js,d.ts,ts}",
    "dist/src/**/constants.{js,d.ts,ts}",
    "dist/src/**/exports.{js,d.ts,ts}",
    "dist/src/shared/dto-modification",
    "dist/src/i18n"
  ],
  "scripts": {
    "ci": "npm install --legacy-peer-deps && npx prisma generate && npm run generate-ts-schema && npm run build && npx prisma generate && npm run format && npm run lint && npm run check-exports",
    "build": "nest build",
    "postinstall": "patch-package",
    "format": "prettier --write \"src/**/*.ts\" \"types/**/*.ts\"",
    "check-format": "prettier --check \"src/**/*.ts\"",
    "check-exports": "attw --pack --profile node16 .",
    "local-release": "changeset version && changeset publish",
    "commit-schema": "git add ./types/schema.d.ts swagger.yaml && git commit -m \"Update schema\"",
    "start": "npx prisma generate && npx nest start",
    "dev": "rm -r ./dist && npx prisma generate && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test,types}/**/*.ts\" --fix",
    "generate-ts-schema": "npx ts-node -r tsconfig-paths/register src/main.swagger.ts && npx openapi-typescript ./swagger.yaml -o ./types/schema.ts",
    "prepare": "husky",
    "prepublishOnly": "npm run ci"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^2.0.10",
    "@anatine/zod-openapi": "^2.2.6",
    "i18n-js": "^4.4.3",
    "libphonenumber-js": "^1.12.6",
    "mercadopago": "^2.4.0",
    "patch-package": "^8.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@changesets/cli": "^2.27.7",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/testing": "^11.0.12",
    "@openapitools/openapi-generator-cli": "^2.18.4",
    "@pdfme/common": "^5.3.13",
    "@pdfme/generator": "^5.3.13",
    "@pdfme/schemas": "^5.3.13",
    "@prisma/client": "^6.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/json-to-pretty-yaml": "^1.2.1",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "country-state-city": "^3.2.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "exceljs": "^4.4.0",
    "fast-csv": "^5.0.2",
    "globals": "^15.10.0",
    "husky": "^9.0.11",
    "i18n-js": "^4.4.3",
    "json-to-pretty-yaml": "^1.2.2",
    "jszip": "^3.10.1",
    "lint-staged": "^15.2.5",
    "luxon": "^3.5.0",
    "multer": "^1.4.5-lts.1",
    "openapi-typescript": "^7.0.0-rc.0",
    "openapi3-ts": "^4.4.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.0",
    "prisma": "^6.3.1",
    "reflect-metadata": "^0.2.0",
    "resend": "^4.1.2",
    "source-map-support": "^0.5.21",
    "string-comparison": "^1.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "main": "./dist/types/index.ts",
  "exports": {
    ".": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "default": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./schema": {
      "default": "./dist/src/schema/exports.js",
      "types": "./dist/src/schema/exports.d.ts"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
